<script type="module" src="user.js"></script>
<div id="sign" class="content-section">
  <form action="/user/sign" method="post">
    <div>
      <label for="ID">ID:</label>
      <input type="text" id="sign_id" name="sign_id" required>
    </div>
    <div>
      <label for="email">Email:</label>
      <input type="email" id="sign_email" name="sign_email" required>
    </div>
    <div>
      <label for="password">Password:</label>
      <input type="password" id="sign_password" name="sign_password" required>
    </div>
    <button type="button" id="signUp">Sign Up</button>
  </form>
  <button id="generateKey">generate</button>
</div>
